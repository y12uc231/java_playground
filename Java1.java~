import java.util.*;
class Java1
{
  static int t;
  
  public static void main(String args[])
  {
      
      System.out.println("Hi,How are you?="+t);
      Scanner s=new Scanner(System.in);
      t=s.nextInt();
      System.out.println("Hi,How are you?="+t);
      Scanner sc=new Scanner("my_file");
      t=sc.nextInt();
      System.out.println("Hi,How are you?="+t);
  }
}